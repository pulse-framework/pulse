{
  "name": "pulse-framework",
  "version": "2.2.0",
  "description": "Global state and logic framework for reactive Javascript applications.",
  "main": "dist/pulse.min.js",
  "module": "dist/pulse.esm.min.js",
  "scripts": {
    "test": "mocha --exit",
    "test-debug": "mocha --inspect-brk",
    "build": "echo \"Building Pulse Framework...\" && rollup -c build/rollup.prod.config.js",
    "build:publish": "echo \"Building Pulse Framework...\" && rollup -c build/rollup.prod.config.js && npm publish",
    "dev": "rollup -c build/rollup.dev.config.js --watch",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs --dest ./docs/dist/",
    "now-build": "npm run docs:build"
  },
  "author": "Jamie Pine",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pulse-framework/pulse"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^4.0.0",
    "jest": "^24.7.1",
    "prettier": "1.16.3",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3",
    "vuepress": "^1.0.2",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "rollup-plugin-uglify-es": "0.0.1"
  }
}
